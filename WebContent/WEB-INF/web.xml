<web-app>

	<servlet>
		<servlet-name>login-servlet</servlet-name>
		<servlet-class>com.TPWeb.servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login-servlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>calcul-servlet</servlet-name>
		<servlet-class>com.TPWeb.servlets.CalculServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>calcul-servlet</servlet-name>
		<url-pattern>/calcul</url-pattern>
	</servlet-mapping>
</web-app>