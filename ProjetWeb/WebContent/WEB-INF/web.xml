<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>ProjetWeb</display-name>
  <welcome-file-list>
    <welcome-file>Connexion.jsp</welcome-file>
  </welcome-file-list>
	<servlet>
		<servlet-name>connexion-servlet</servlet-name>
		<servlet-class>com.ENSAK.TPWeb.Connexion</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>connexion-servlet</servlet-name>
		<url-pattern>/Connexion</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>controle-servlet</servlet-name>
		<servlet-class>com.ENSAK.TPWeb.ControleEtudiant</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>controle-servlet</servlet-name>
		<url-pattern>/ControleEtudiant</url-pattern>
	</servlet-mapping>
</web-app>